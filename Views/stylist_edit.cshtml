<h1>Best Restaurants</h1>
 <hr />
<h2>Edit Cuisine: @Model["cuisine"].GetType()</h2>
<form action='/cuisine/edit/@Model["cuisine"].GetId()' method="post">
  <input type="hidden" name="_method" value="PATCH">
  <input name="cuisine-id" type="hidden" value='@Model["cuisine"].GetId()'>
  <label for="cuisine-type">Cuisine type</label>
  <input id="cuisine-type" name="cuisine-type" type="text">
  <button type="submit">Submit</button>
</form>
<h4><a href="/">Return home</a></h4>
